def solution(skill,skill_trees):
    answer=0
    for i in skill_trees:
        list=[]
        compare=True
        for j in range(0,len(i)):
            if i[j] in skill:
                list.append(i[j])
        for k in range(len(list)):
            if(skill[k]!=list[k]):
                compare=False
                break
        if(compare):
            answer+=1
    return answer
