def solution(s):
    start_count=0
    end_count=0
    answer=True
    for i in range(0,len(s)):
        if(s[i]=='('):
            start_count+=1
        else:
            end_count+=1
        if end_count>start_count:
            answer=False
            break
    if(start_count!=end_count):
        answer=False
    return answer
